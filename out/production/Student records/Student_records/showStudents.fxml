                </HBox>
            </graphic>
        </Button>
        <TableView fx:id="table" layoutX="10.0" layoutY="41.0" prefHeight="486.0" prefWidth="825.0">
            <columns>
                <TableColumn fx:id="col_id" prefWidth="74.0" text="opiskelija_id" />
                <TableColumn fx:id="col_name" prefWidth="116.0" text="Etunimi" />
                <TableColumn fx:id="col_surname" prefWidth="129.0" text="Sukunimi" />
                <TableColumn fx:id="col_email" prefWidth="168.0" text="Email" />
                <TableColumn fx:id="col_phone" prefWidth="156.0" text="Puhelin" />
