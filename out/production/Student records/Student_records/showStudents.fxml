                <HBox alignment="CENTER_RIGHT" prefHeight="28.0" prefWidth="84.0">
                    <children>
                        <ImageView fitHeight="25.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@/images/student.png">

                            </Image>
                        </ImageView>
                        <Label prefHeight="28.0" prefWidth="89.0" text="Näytä opiskelijat" />
                    </children>
                </HBox>
            </graphic>
        </Button>
    </children>
</AnchorPane>
